    """Calculate stuff"""
    # Unpack parameters for each QD
    qd1_size, Vvb, mh, mh = qd1_prop
    qd2_size, Vvb, mh, mh = qd2_prop
    bi = ["VB1", "VB2"]; bf = ["CB1", "CB2"]
