def max_rectangle_area(lmd, T):
    # this function calculates maximum rectangular area under given plot

    # a primeira coluna da matriz corresponde ao comprimento de onda (x) e a
    # segunda coluna  reflexo (y)
    max_area = 0
    i = 0
    for i in range(len(lmd)):
        x1 = lmd[i]
        y1 = T[i]
        j = i + 1
        while j <= len(lmd) - 1:
            if T[i] <= T[i + 1]:
                # bloquear o scan se a funo estiver num ponto com declive negativo
                x_scan = lmd[j]
                y_scan = T[j]
                if (y_scan <= y1 + y1 * 0.2) or (
                        y_scan >= y1 - y1 * 0.2):  # meter tolerncia
                    if j < len(lmd) - 1 and T[j + 1] >= T[j]:
                        j = j + 1
                    else:
                        # quando no h um mnimo local mas tambm no  o
                        # ltimo ponto
                        x2 = x_scan
                        area = (x2 - x1) * y1
                        j = len(lmd)  # cortar ciclo
                else:
                    if j == len(lmd) - 1:
                        # quando o x_scan alcana o limite da funo
                        x2 = x_scan
                        area = (x2 - x1) * y1
                    j = j + 1
            else:
                area = 0
                j = j + 1
        if area > max_area:
            max_area = area
            max_matrix = [(x1, y1), (x2, y1)]
        i = i + 1
    print(max_area)
    print(max_matrix)
