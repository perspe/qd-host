def update_step(it_num, total_it):
    """Helper function to print the iteration number
    in a for loop"""
    if it_num == total_it:
        print(f"\rIteration {it_num:04d}/{total_it:04d}")
    else:
        print(f"\rIteration {it_num:04d}/{total_it:04d}", end="")
    pass
