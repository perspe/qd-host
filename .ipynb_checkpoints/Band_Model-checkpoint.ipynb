            ax[i, k].tick_params(labelsize=14)
            if k == 0:
                ax[i, k].set_ylabel("Absorption Coef (nm$^3$/cm)", fontsize=16)
            if i == 2:
                ax[i, k].set_xlabel("Energy (eV)", fontsize=16)
    if save:
        plt.savefig("Absorption_Coefs.svg", transparent=True)
