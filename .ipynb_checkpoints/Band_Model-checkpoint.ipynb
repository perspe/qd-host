def V_offset(Host_Eg, QD_Eg, offset_ratio):
    """ Return the adequately offseted Vcv/Vvb values """
    Vcb = (Host_Eg - QD_Eg)*offset_ratio
    Vvb = (Host_Eg - QD_Eg)*(1-offset_ratio)
    return Vcb, Vvb
